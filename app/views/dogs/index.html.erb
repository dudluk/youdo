<% provide(:title, 'Dogs') %>

<div class="row">
  <div class="panel panel-default">
    <table class="table">
      <thead>
      <tr>
        <th>Dog name</th>
        <th>Breed</th>
        <th>Owner name </th>
        <th>Owner email </th>
        <th>Registered until</th>
      </tr>
      </thead>
      <tbody>
      <%= render @dogs %>
      </tbody>
    </table>
  </div>
  <%= will_paginate  %>
</div>